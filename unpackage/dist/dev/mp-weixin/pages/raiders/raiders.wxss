
.list_img{
	width: 100%;
}
.list_title{
	position: absolute;
	top: 30rpx;
	left: 40rpx;
	font-size: 40rpx;
	color: #ffffff;
	z-index: 10;
}
.list{
	width: 700rpx;
	border-radius: 20rpx;
	position: relative;
}
.title{
	font-size: 40rpx;
	margin-left: 25rpx;
}
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
page {
	width: 100%;
	height: 100%;
	background: -webkit-gradient(linear, left top, left bottom, from(#ffffff), to(#94bff8));
	background: -o-linear-gradient(#ffffff, #94bff8);
	background: linear-gradient(#ffffff, #94bff8);
	overflow: hidden;
}
view, text {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.body {
	width: 100%;
	height: 100%;
	overflow-y: scroll;
	padding-bottom: 20rpx;
	overflow-x: hidden;
}
.body::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}

